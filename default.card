  view7:
    type: element
    title: Liquid View 6
    xtype: at-core-view
    view: |-
      <at-carbon-tabs class="mx" id="file-source-selection" selected="{% if _tab %}{{_tab}}{% else %}0{% endif %}" style="margin-bottom:0px!important">
      	<at-carbon-tab id="0" at-click-action="used">Recent</at-carbon-tab>
      	<at-carbon-tab id="1" at-click-action="trending">Trending</at-carbon-tab>
      	<at-carbon-tab id="2" at-click-action="shared">Shared</at-carbon-tab>
      	<at-carbon-tab id="3" at-click-action="email">Email</at-carbon-tab>
      </at-carbon-tabs>

      {% if items.length == 0 and _action == "firstpage" %}
      	<div class="p layout-self-stretch font-title grey" style="text-align: center;">
      		No results.
      	</div>
      {% endif %}

      <style>
        #file-source-selection at-carbon-tab {
        	padding: 0px !important;
          margin-right: 8px !important;
        }
      </style>
    modelRoot: state.model
    model: 
    value: 
